{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": [
        "plugin:antony/recommended"
    ],
    "ignorePatterns": [
        "!**/*"
    ],
    "plugins": [
        "antony"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "sonarjs/no-nested-template-literals": "off"
    }
}